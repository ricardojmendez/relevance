{
  "name": "Relevance",
  "version": "0.2",
  "browser_action": {
    "default_title": "Show history",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs", "storage", "idle", "alarms", "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["https://*.startpage.com/*", "https://duckduckgo.com/*"],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}

